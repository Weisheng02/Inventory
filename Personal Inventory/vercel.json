{
  "version": 2,
  "buildCommand": "vite build",
  "devCommand": "vite",
  "installCommand": "npm install --no-audit --no-fund",
  "outputDirectory": "dist",
  "builds": [
    { "src": "server/api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server/api/$1" }
  ]
}


